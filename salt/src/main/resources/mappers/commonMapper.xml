<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="salt.mapper.commonMapper">
    <insert id="addAccessLog" parameterType="String">
        INSERT INTO T_ACCESS_LOG ( seq, id, flgKey )
        VALUES  ( SEQ_T_ACCESS_LOG.NEXTVAL, #{id}, #{flgKey} )
    </insert>
</mapper>
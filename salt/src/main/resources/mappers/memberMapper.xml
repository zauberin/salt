<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="salt.mapper.memberMapper">

    <select id="getMemberInfo" resultType="com.winterfell.pilot.domain.MemberVO">
        SELECT ID
              , NAME
              , EMAIL
              , PHONE
              , ADDR
              , BIRTHDAY
              , PWD
              , REGDATE
              , MODDATE
              , LOGINCNT
              , ETC
          FROM T_MEMBER
         WHERE ID = #{id}
    </select>

    <select id="getMemberList" resultType="com.winterfell.pilot.domain.MemberVO">
        SELECT ID
        , NAME
        , EMAIL
        , PHONE
        , ADDR
        , BIRTHDAY
        , PWD
        , REGDATE
        , MODDATE
        , LOGINCNT
        , ETC
        FROM T_MEMBER
        WHERE ID = #{id}
    </select>



</mapper>